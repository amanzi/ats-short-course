      </ParameterList>
    </ParameterList>
    <ParameterList name="snow" type="ParameterList">
      <Parameter name="mesh type" type="string" value="aliased" />
      <ParameterList name="aliased parameters" type="ParameterList">
        <Parameter name="target" type="string" value="surface" />
